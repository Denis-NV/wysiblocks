{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "page_blocks": {
      "via": "page",
      "collection": "page-block"
    },
    "uri": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "is_live": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "unpublished": {
      "type": "boolean",
      "default": false
    },
    "protected": {
      "type": "boolean",
      "default": false
    },
    "site": {
      "model": "site",
      "via": "pages"
    },
    "live": {
      "type": "component",
      "repeatable": false,
      "component": "data-sets.page-data-set"
    },
    "draft": {
      "type": "component",
      "repeatable": false,
      "component": "data-sets.page-data-set"
    }
  }
}
