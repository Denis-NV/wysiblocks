{
  "kind": "collectionType",
  "collectionName": "page_blocks",
  "info": {
    "name": "Page Block"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "component": {
      "type": "string",
      "required": true
    },
    "page": {
      "model": "page",
      "via": "page_blocks"
    },
    "is_live": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "unpublished": {
      "type": "boolean",
      "default": false
    },
    "site": {
      "model": "site",
      "via": "page_blocks"
    },
    "live": {
      "type": "component",
      "repeatable": false,
      "component": "data-sets.data-set"
    },
    "draft": {
      "type": "component",
      "repeatable": false,
      "component": "data-sets.data-set"
    }
  }
}
