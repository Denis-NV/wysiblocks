{
  "kind": "collectionType",
  "collectionName": "nav_items",
  "info": {
    "name": "Nav Item"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "is_live": {
      "type": "boolean",
      "default": false
    },
    "unpublished": {
      "type": "boolean",
      "default": false
    },
    "page": {
      "unique": true,
      "model": "page"
    },
    "site": {
      "model": "site",
      "via": "nav_items"
    },
    "live": {
      "type": "component",
      "repeatable": false,
      "component": "data-sets.nav-item-data-set"
    },
    "draft": {
      "type": "component",
      "repeatable": false,
      "component": "data-sets.nav-item-data-set"
    }
  }
}
